{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch PSCleaner",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "envFile": "${workspaceFolder}/.env",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "runtimeArgs": [
        "${workspaceFolder}"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Attach to Functions",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "preLaunchTask": "func: host start"
    },
    {
      "name": "Attach to Node Functions",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "preLaunchTask": "func: host start"
    }
  ],
  "compounds": [
    {
      "name": "Debug Azure",
      "configurations": ["Attach to Functions"]
    }
  ]
}