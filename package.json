{
  "author": "NEL",
  "dependencies": {
    "@buckneri/js-lib-selection": "0.0.6",
    "@buckneri/string": "^0.3.10",
    "@buckneri/web-components": "^0.2.4",
    "chokidar": "^3.4.0",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^5.1.1",
    "electron-window-state": "^5.0.3",
    "esm": "^3.2.25",
    "fast-csv": "^4.1.4",
    "file-url": "^3.0.0",
    "fs-extra": "^9.0.0",
    "he": "^1.2.0",
    "jschardet": "^2.1.1",
    "json-stringify-pretty-compact": "^2.0.0",
    "make-dir": "^3.1.0",
    "minimist": "^1.2.5",
    "node-abi": "^2.16.0",
    "node-pre-gyp": "^0.14.0",
    "username": "^5.1.0",
    "uuid": "^8.0.0",
    "valid-filename": "^3.1.0",
    "wink-pos-tagger": "^2.2.2"
  },
  "description": "Process CSV files by identifying and removing personal sensitive text",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.45",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.45",
    "@electron/typescript-definitions": "^8.7.4",
    "@types/bluebird": "^3.5.31",
    "@types/chokidar": "^2.1.3",
    "@types/debounce": "^1.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/he": "^1.1.1",
    "@types/node": "^12.12.39",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "electron": "^8.3.0",
    "electron-rebuild": "^1.11.0",
    "eslint": "^7.0.0",
    "eslint-plugin-jsdoc": "^25.4.1",
    "jsdoc": "^3.6.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.9.2"
  },
  "keywords": [
    "nodejs",
    "typescript"
  ],
  "license": "MIT",
  "main": "backend/build/app.js",
  "name": "pscleaner",
  "productName": "PSCleaner",
  "homepage": "https://github.com/NELCSU/PSCleaner",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NELCSU/pscleaner.git"
  },
  "scripts": {
    "build": "npm-run-all --parallel build-client build-backend",
    "build-backend": "tsc --project ./backend/tsconfig.json",
    "build-client": "tsc --project ./client/tsconfig.json",
    "eslint": "eslint -c .eslintrc.json --ext .ts --ext .js ./src",
    "make": "npm run wipe-all && npm run update-webc && npm run make-w32 && npm run make-w64",
    "make-w32": "electron-forge make --platform=win32 --arch=ia32",
    "make-w64": "electron-forge make",
    "package": "npm run package-w64 && npm run package-w32",
    "package-w64": "electron-forge package",
    "package-w32": "electron-forge package --platform=win32 --arch=ia32",
    "publish": "electron-forge publish",
    "start": "electron-forge start",
    "update-all": "npm run update-elec && npm run wipe-webc && npm run update-webc",
    "update-elec": ".\\node_modules\\.bin\\electron-rebuild.cmd && npm rebuild",
    "update-webc": "ncp node_modules/@buckneri/web-components client/lib/web-components",
    "watch": "tsc -w",
    "wipe-all": "npm run wipe-out && npm run wipe-webc",
    "wipe-out": "rimraf out",
    "wipe-webc": "rimraf client/lib/web-components"
  },
  "version": "0.8.0",
  "bugs": {
    "url": "https://github.com/NELCSU/PSCleaner/issues"
  }
}
