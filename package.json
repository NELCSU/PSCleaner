{
  "author": "NHS NEL CSU",
  "dependencies": {
    "@buckneri/collections": "^0.1.6",
    "@buckneri/string": "^0.7.0",
    "@buckneri/web-components": "^0.3.10",
    "chokidar": "^3.5.1",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "electron-log": "^4.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.1",
    "electron-window-state": "^5.0.3",
    "esm": "^3.2.25",
    "fast-csv": "^4.3.6",
    "file-url": "^3.0.0",
    "fs-extra": "^9.0.1",
    "he": "^1.2.0",
    "jschardet": "^2.2.1",
    "json-stringify-pretty-compact": "^2.0.0",
    "make-dir": "^3.1.0",
    "minimist": "^1.2.5",
    "tslib": "^2.1.0",
    "username": "^5.1.0",
    "uuid": "^8.3.2",
    "valid-filename": "^3.1.0"
  },
  "description": "Process CSV files by identifying and removing personal sensitive text",
  "devDependencies": {
    "@electron/typescript-definitions": "^8.8.0",
    "@types/bluebird": "^3.5.33",
    "@types/debounce": "^1.2.0",
    "@types/fs-extra": "^9.0.6",
    "@types/he": "^1.1.1",
    "@types/node": "^12.19.14",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "bl": "^4.0.3",
    "electron": "^11.2.0",
    "electron-builder": "^22.9.1",
    "electron-builder-squirrel-windows": "^22.10.3",
    "electron-wix-msi": "^3.0.6",
    "eslint": "^7.18.0",
    "eslint-plugin-jsdoc": "^31.0.7",
    "exe-icon-extractor": "^1.0.8",
    "jsdoc": "^3.6.6",
    "ncp": "^2.0.0",
    "node-abi": "^2.19.3",
    "node-gyp": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "nodejs",
    "typescript"
  ],
  "license": "MIT",
  "main": "backend/build/app.js",
  "name": "pscleaner",
  "productName": "PSCleaner",
  "homepage": "https://github.com/NELCSU/PSCleaner",
  "repository": {
    "type": "git",
    "url": "https://github.com/NELCSU/pscleaner.git"
  },
  "scripts": {
    "build": "npm-run-all --parallel build-client build-backend",
    "build-backend": "tsc --project ./backend/tsconfig.json",
    "build-client": "tsc --project ./client/tsconfig.json",
    "deploy": "electron-builder build --win --publish always",
    "make": "npm run wipe-all && npm run update-webc && npm run make-build",
    "make-build": "electron-builder build --win --publish never",
    "update-all": "npm run update-elec && npm run wipe-webc && npm run update-webc",
    "update-elec": "./node_modules/.bin/electron-rebuild.cmd && npm rebuild",
    "update-webc": "ncp node_modules/@buckneri/web-components client/lib/web-components",
    "watch": "tsc -w",
    "wipe-all": "npm run wipe-out && npm run wipe-webc",
    "wipe-out": "rimraf out",
    "wipe-webc": "rimraf client/lib/web-components"
  },
  "version": "0.9.7",
  "bugs": {
    "url": "https://github.com/NELCSU/PSCleaner/issues"
  }
}
